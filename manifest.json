{
  "name": "Dark Desmos",
  "version": "1.0.2",
  "manifest_version": 3,
  "description": "Dark mode for Desmos.",
  "content_scripts": [
	  {
	    "matches": [
	      "https://*.desmos.com/calculator*",
	      "https://*.desmos.com/3d*",
		  "https://*.desmos.com/geometry*"
		],
	    "css": [
	      "css/graphing.css"
	    ]
	  },
	  {
	    "matches": [
	      "https://*.desmos.com/scientific*",
		  "https://*.desmos.com/fourfunction*",
		  "https://*.desmos.com/matrix*"
	    ],
	    "css": [
	      "css/scientific.css"
	    ]
	  },
	  {
	    "matches": [
	      "https://*.desmos.com/*"
	    ],
	    "css": [
	      "css/home.css"
	    ]
	  }
  ],
  "icons": {
  	"128":"images/icon.png"
  }
}
